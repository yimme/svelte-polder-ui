{
  "name": "svelte-polder-ui",
  "version": "1.0.0",
  "description": "Component Library for Svelte",
  "private": true,
  "scripts": {
    "lib:storybook": "pnpm -F polder run storybook",
    "lib:build": "pnpm -F polder run build",
    "lib:check": "pnpm -F polder run check",
    "docs:dev": "pnpm -F @polder/docs run dev",
    "docs:build": "pnpm -F @polder/docs run build",
    "build": "pnpm run lib:check && pnpm run docs:build"
  },
  "keywords": [
    "svelte",
    "component library",
    "ui library",
    "svelte kit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yimme/svelte-polder-ui"
  },
  "author": "Jimme",
  "license": "MIT",
  "devDependencies": {
    "pnpm": "^7.15.0"
  }
}
